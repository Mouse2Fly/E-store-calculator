modules = ["web", "php-8.2", "nodejs-20", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33345
externalPort = 5000

[[ports]]
localPort = 36257
externalPort = 3003

[[ports]]
localPort = 36635
externalPort = 3002

[[ports]]
localPort = 36755
externalPort = 6000

[[ports]]
localPort = 42069
externalPort = 5173

[[ports]]
localPort = 42469
externalPort = 3000

[[ports]]
localPort = 43475
externalPort = 3001

[[ports]]
localPort = 44595
externalPort = 4200

[deployment]
deploymentTarget = "autoscale"
run = ["python3", "server.py"]
